.card
  background-color white
  border-radius 3px
  padding 6px 8px
  margin-top 10px
  box-shadow 0 1px 0 rgba(0, 0, 0, 0.25)
  font-family "Helvetica Neue", Arial, Helvetica, sans-serif
  color: #444

.header
  font-size 14px
  margin-bottom 2px
  display inline-block

.buttons
  float right

  & a
    color red
    margin 0 3px
    cursor pointer